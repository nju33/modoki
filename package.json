{
  "name": "modoki",
  "description": "Get 'modoki' value immidiately",
  "version": "0.0.2",
  "types": "dist/modoki.d.ts",
  "main": "dist/modoki.js",
  "umd:main": "dist/modoki.umd.js",
  "module": "dist/modoki.m.js",
  "source": "src/modoki.ts",
  "repository": "git@github.com:nju33/modoki.git",
  "author": "nju33 <nju33.ki@gmail.com>",
  "publishConfig": {
    "access": "public"
  },
  "license": "MIT",
  "keywords": [
    "dummy"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "yarn build",
    "build": "microbundle -i src/modoki.ts --name modoki",
    "test": "jest",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "devDependencies": {
    "@geekcojp/tslint-config": "^0.1.2",
    "@types/jest": "^22.2.0",
    "coveralls": "^3.0.0",
    "jest": "^22.4.2",
    "microbundle": "^0.4.4",
    "prettier": "^1.11.1",
    "ts-jest": "^22.4.1",
    "tslint": "^5.9.1",
    "typescript": "^2.7.2"
  },
  "dependencies": {},
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "testMatch": [
      "**/*.test.ts"
    ]
  }
}
